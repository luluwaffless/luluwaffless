{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/users",
      "dest": "index.js"
    },
    {
      "src": "/thumbnail",
      "dest": "index.js"
    },
    {
      "src": "/badges/(.*)",
      "dest": "index.js"
    },
    {
      "src": "/dates/(.*)",
      "dest": "index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}